{
  "name": "snyk-pnpm-deptree-api-tool",
  "description": "use snyk-nodejs-lockfile-parser to build a deptree and scan it via the api",
  "main": "dist/index.js",
  "bin": {
    "snyk-pnpm-deptree-api-tool": "dist/index.js"
  },
  "scripts": {
    "format:check": "prettier --check '{''{lib,test}/!(fixtures)/**/*,*}.{js,ts,json,yml}'",
    "format": "prettier --write '{''{lib,test}/!(fixtures)/**/*,*}.{js,ts,json,yml}'",
    "lint": "npm run format:check && npm run lint:eslint",
    "lint:eslint": "eslint --cache '{lib,test}/**/*.ts'",
    "test": "snyk test && npm run lint && npm run test:unit",
    "test:unit": "jest",
    "test:coverage": "npm run test:unit -- --coverage",
    "test:watch": "tsc-watch --onSuccess 'npm run test:unit'",
    "build": "tsc",
    "build-watch": "tsc -w",
    "prepare": "npm run build",
    "snyk-test": "snyk test",
    "pkg-binaries": "npx pkg . -t node12-linux-x64,node12-macos-x64,node12-win-x64 --out-path ./dist/binaries"
  },
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/snyk-tech-services/snyk-pnpm-dephtree-api-tool"
  },
  "author": "Snyk Tech Services",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=12"
  },
  "files": [
    "bin",
    "dist"
  ],
  "homepage": "https://github.com/snyk-tech-services/snyk-pnpm-dephtree-api-tool#readme",
  "dependencies": {
    "@babel/preset-env": "^7.14.8",
    "@babel/runtime": "^7.14.8",
    "@pnpm/logger": "^3.2.3",
    "@snyk/configstore": "^3.2.0-rc1",
    "@snyk/dep-graph": "^1.28.1",
    "axios": "^0.21.4",
    "babel-helpers": "^6.24.1",
    "core-js": "^2.6.12",
    "debug": "^4.1.1",
    "lodash.clone": "^4.5.0",
    "lodash.constant": "^3.0.0",
    "lodash.filter": "^4.6.0",
    "lodash.foreach": "^4.5.0",
    "lodash.isempty": "^4.4.0",
    "lodash.isequal": "^4.5.0",
    "lodash.isfunction": "^3.0.9",
    "lodash.isundefined": "^3.0.1",
    "lodash.keys": "^4.2.0",
    "lodash.map": "^4.6.0",
    "lodash.reduce": "^4.6.0",
    "lodash.size": "^4.2.0",
    "lodash.transform": "^4.6.0",
    "lodash.union": "^4.6.0",
    "lodash.values": "^4.3.0",
    "nock": "^13.1.1",
    "object-hash": "^2.0.3",
    "regenerator-runtime": "^0.10.5",
    "snyk": "^1.685.0",
    "snyk-config": "^3.0.0",
    "snyk-nodejs-lockfile-parser-ts-fork": "^1.36.0",
    "source-map-support": "^0.5.16",
    "test": "^0.6.0",
    "tslib": "^1.10.0",
    "webpack": "^5.47.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@semantic-release/exec": "^5.0.0",
    "@types/jest": "^25.2.3",
    "@types/lodash": "^4.14.149",
    "@types/lodash.constant": "^3.0.6",
    "@types/lodash.foreach": "^4.5.6",
    "@types/lodash.isequal": "^4.5.5",
    "@types/lodash.isundefined": "^3.0.6",
    "@types/node": "^12.20.17",
    "@types/object-hash": "^2.1.1",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^25.5.1",
    "babel-loader": "^8.2.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "jest": "^25.5.4",
    "pkg": "^5.3.2",
    "prettier": "^1.19.1",
    "semantic-release": "^17.4.7",
    "ts-jest": "^25.5.1",
    "ts-node": "8.6.2",
    "tsc-watch": "^4.1.0",
    "typescript": "^3.7.5"
  },
  "pkg": {
    "scripts": [
      "dist/**/*.js"
    ]
  },
  "version": "1.0.4"
}
